<h1 style="margin-bottom: 40px;"> REGISTER</h1>
{{err}}
<div class="register">
    {{#if signed}}
     <h2>User not found, plase register yourself</h2>
  {{/if}}
    <form method="POST">
        <label for="first">First Name</label>
        <input class="input-list" type="text" name="first"></input>
        <label for="last">Last Name</label>
        <input class="input-list" type="text" name="last"></input>
        <label for="email">Email</label>
        <input class="input-list" type="email" name="email"></input>
        <label for="password">Password</label>
        <input class="input-list" type="password" name="password"></input>
        {{! <input name="password" value="trustno1" /> }}
        
    <div class="div-button">
        <button class="submit" type="submit">REGISTER</button>
    </div>
    </form>
      <a href="/login"><button class="submit" type="submit">LOGIN</button></a>  
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="./request.js"></script>
</div>